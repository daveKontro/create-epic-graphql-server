{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch GraphQL Server (tsx)",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/server/src/index.ts",
      "runtimeExecutable": "tsx",
      "runtimeArgs": ["watch"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "cwd": "${workspaceFolder}",
      "protocol": "inspector"
    }
    
  ]
}